x--;\n    },\n    showResult: function() {\n      for (var index = 0; index < this.answer.length; index++) {\n        if (\n          !(this.answer[index] == null) &&\n          this.answer[index].status === true\n        ) {\n          this.countCorrectAnswer++;\n        }\n      }\n      this.result = true;\n      this.next();\n    },\n    mixArray: function (arr) {\n      let lngth = arr.length, randomPosition, tmpEl; \n\n      while(lngth){\n        randomPosition = Math.floor(Math.random() * lngth--);\n\n        tmpEl = arr[lngth];\n        arr[lngth] = \n      }\n\n\n    },\n  },\n  mounted() {\n    axios.get('data.json').then((response) => {\n      this.datas = response.data.use;\n      this.mixArray(this.datas);\n    });\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: flex;\n  justify-content: center;\n  margin: 0 10px;\n}\n.error{\n  color: rgb(211, 15, 15);\n  font-size: 9px;\n  font-weight: 900;\n}\nbutton{\n  margin: 0 10px;\n}\n</style>\n"]}]}